<!DOCTYPE html>
<html lang="en">
<head>
   @await Html.PartialAsync("MetatagPartial")
   @await Html.PartialAsync("CssPartial")
</head>
<body style="background-color: #454853">
    @await Html.PartialAsync("HeaderPartial")
    
        <main role="main" class="pb-3" style="padding-top:120px">
            @RenderBody()
        </main>
       
  

    @await Html.PartialAsync("FooterPartial")
    @await Html.PartialAsync("ScriptPartial")
    @RenderSection("Scripts", required: false)
</body>
</html>
